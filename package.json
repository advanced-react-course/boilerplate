  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint ./src/ --max-warnings=0",
    "lint:fix": "npm run lint -- --cache --fix",
    "lint:commit": "commitlint",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint src/**/styles.ts",
    "lint:type": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "prestorybook:start": "rimraf node_modules/.cache/storybook",
    "prestorybook:build": "npm run test:generate-output",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:test": "test-storybook",
    "generate": "plop --plopfile ./.plop/plopfile.js",
    "prepare": "is-ci || husky install"
